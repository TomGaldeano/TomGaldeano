package claseU3;

import java.util.Scanner;

public class H2E16 {
	public static void main(String[] args) {
		int i = 0,num=0;
		Scanner entrada = new Scanner(System.in);
		System.out.println("Numero para hacer factorial");
		num= entrada.nextInt();
		entrada.close();
		for (i=1;i<=num;i++) {
			System.out.println("*");
		}	
		}
}
